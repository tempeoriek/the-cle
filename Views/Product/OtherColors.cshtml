@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
        @*<div class="item-box">@Html.Partial("_ProductBox", item)</div>*@
        <a href="@Url.RouteUrl("Product", new { SeName = item.SeName})" class="clean">
            <div class="attribute-square" itemprop="color" style="background-color:@item.ColorHex"></div>
        </a>
        

    }
}